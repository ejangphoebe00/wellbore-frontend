{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Wellbore-Store": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/Wellbore-Store",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "src/assets/assets/global/plugins/font-awesome/css/font-awesome.min.css",
              "src/assets/assets/global/plugins/simple-line-icons/simple-line-icons.min.css",
              "src/assets/assets/global/plugins/bootstrap/css/bootstrap.min.css",
              "src/assets/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css",
              "src/assets/assets/global/plugins/bootstrap-daterangepicker/daterangepicker.min.css",
              "src/assets/assets/global/plugins/morris/morris.css",
              "src/assets/assets/global/plugins/fullcalendar/fullcalendar.min.css",
              "src/assets/assets/global/plugins/jqvmap/jqvmap/jqvmap.css",
              "src/assets/assets/global/css/components-rounded.min.css",
              "src/assets/assets/global/css/plugins.min.css",
              "src/assets/assets/layouts/layout/css/layout.min.css",
              "src/assets/assets/layouts/layout/css/themes/darkblue.min.css",
              "src/assets/assets/layouts/layout/css/custom.min.css",
              "src/assets/assets/global/plugins/datatables/datatables.min.css",
              "src/assets/assets/global/plugins/datatables/plugins/bootstrap/datatables.bootstrap.css",
              "src/assets/assets/global/plugins/bootstrap-datepicker/css/bootstrap-datepicker3.min.css"
              
            ],
            "scripts": [
               "node_modules/jquery/dist/jquery.js",
                "node_modules/datatables.net/js/jquery.dataTables.js",
                "src/assets/assets/global/scripts/app.min.js",
                "src/assets/assets/global/plugins/js.cookie.min.js",
                "src/assets/assets/global/plugins/js.cookie.min.js",
                "src/assets/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js",
                "src/assets/assets/global/plugins/jquery.blockui.min.js",
                "src/assets/assets/global/plugins/moment.min.js",
                "src/assets/assets/global/plugins/morris/morris.min.js",
                "src/assets/assets/global/plugins/morris/raphael-min.js",
                "src/assets/assets/global/plugins/counterup/jquery.waypoints.min.js",
                "src/assets/assets/global/plugins/counterup/jquery.counterup.min.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/amcharts.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/serial.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/pie.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/radar.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/themes/light.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/themes/patterns.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/amcharts.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/serial.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/pie.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/radar.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/themes/light.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/themes/patterns.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/themes/chalk.js",
                "src/assets/assets/global/plugins/amcharts/ammap/ammap.js",
                "src/assets/assets/global/plugins/amcharts/ammap/maps/js/worldLow.js",
                "src/assets/assets/global/plugins/amcharts/amstockcharts/amstock.js",
                "src/assets/assets/global/plugins/amcharts/amcharts/themes/light.js",
                "src/assets/assets/global/plugins/fullcalendar/fullcalendar.min.js",
                "src/assets/assets/global/plugins/horizontal-timeline/horizontal-timeline.js",
                "src/assets/assets/global/plugins/flot/jquery.flot.min.js",
                "src/assets/assets/global/plugins/flot/jquery.flot.resize.min.js",
                "src/assets/assets/global/plugins/flot/jquery.flot.categories.min.js",
                "src/assets/assets/global/plugins/jquery-easypiechart/jquery.easypiechart.min.js",
                "src/assets/assets/global/plugins/jquery.sparkline.min.js",
                "src/assets/assets/global/plugins/jqvmap/jqvmap/jquery.vmap.js",
                "src/assets/assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.russia.js",
                "src/assets/assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.world.js",
                "src/assets/assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.europe.js",
                "src/assets/assets/global/plugins/jquery-easypiechart/jquery.easypiechart.min.js",
                "src/assets/assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.germany.js",
                "src/assets/assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.usa.js",
                "src/assets/assets/global/plugins/jqvmap/jqvmap/data/jquery.vmap.sampledata.js",
                "src/assets/assets/pages/scripts/dashboard.min.js",
                "src/assets/assets/pages/scripts/table-datatables-buttons.min.js",
                "src/assets/assets/layouts/layout/scripts/layout.min.js",
                "src/assets/assets/layouts/layout/scripts/demo.min.js",
                "src/assets/assets/layouts/global/scripts/quick-sidebar.min.js",
                "src/assets/assets/layouts/global/scripts/quick-nav.min.js",
                "src/assets/assets/global/plugins/bootstrap/js/bootstrap.min.js",
                "src/assets/assets/global/plugins/bootstrap-daterangepicker/daterangepicker.min.js",
                "src/assets/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js"


               
                         
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "Wellbore-Store:build:production"
            },
            "development": {
              "browserTarget": "Wellbore-Store:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Wellbore-Store:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "Wellbore-Store"
}
